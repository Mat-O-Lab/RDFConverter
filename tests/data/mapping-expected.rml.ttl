@prefix mail: <http://example.com/mail#>.
@prefix ex: <http://www.example.com/>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@base <http://example.com/ns#>.


<#FromToMaps_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "src/test/resources/example1/example1.xml";
		rml:referenceFormulation ql:XPath;
		rml:iterator "/notes/note"
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://www.example.com/note/{@id}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant mail:when;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:template "{@year}-{@month}-{@day}";
			rr:datatype xsd:date
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant mail:from;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "from";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant mail:to;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "to";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant mail:heading;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "heading";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant mail:body;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "body";
		];
	].


